  <brief_title>Recovery of Patients From COVID-19 After Critical Illness</brief_title>
  <acronym>COVID-Recovery</acronym>
  <official_title>The COVID-Recovery Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Australian and New Zealand Intensive Care Research Centre</agency>
      <agency_class>Other</agency_class>
