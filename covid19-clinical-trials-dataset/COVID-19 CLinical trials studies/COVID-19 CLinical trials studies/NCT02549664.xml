  <eligibility>
    <study_pop>
      <textblock>
        Individuals with Hypertrophic Cardiomyopathy or Long QT Syndrome between the ages of 8-60.&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  Individuals with Hypertrophic Cardiomyopathy or Long QT Syndrome&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  Individuals with Hypertrophic Cardiomyopathy with systolic or diastolic heart failure&#xD;
             which precludes vigorous exercise will not be enrolled (NYHA III/IV)&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>8 Years</minimum_age>
    <maximum_age>60 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
